{
  "name": "Fortune",
  "description": "My app bundled with Express",
  "version": "1.0.0",
  "version_short": "1.0",
  "type": "module",
  "scripts": {
    "docker-build": "build.sh",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "dev-start": "nodemon -r dotenv/config src/app.mjs",
    "update": "git pull",
    "serve": "npm-run-all build start",
    "serve-restart": "npm-run-all update build restart",
    "start": "pm2 start src/app.mjs",
    "restart": "pm2 restart src/app.mjs",
    "stop": "pm2 stop src/app.mjs",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "migrate": "node src/server/models/migration.js up --migrate-all",
    "migration-add": "node src/server/models/migration.js add migration",
    "docs": "apidoc -i src/controllers -o docs/"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-replace": "2.3.1",
    "npm-run-all": "^4.1.5",
    "recto": "4.4.1",
    "rollup": "2.77.4-1",
    "rollup-plugin-css-only": "2.0.0",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-svelte-ssr": "1.0.0",
    "rollup-plugin-terser": "^5.3.1",
    "svelte": "^3.55.1",
    "svelte-router-spa": "^6.0.3",
    "svelte-routing": "1.4.2",
    "svelte-watch-resize": "^1.0.3",
    "sveltestrap": "5.9.0"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "connect-redis": "^6.1.3",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "5.1.0",
    "errorhandler": "1.5.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.0",
    "method-override": "3.0.0",
    "module-alias": "^2.2.2",
    "mysql-migrations": "^1.0.7",
    "mysql-model-router": "^0.0.2",
    "mysql2": "^2.3.3",
    "node-ses": "^3.0.3",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "openid-client": "^5.1.10",
    "pm2": "^5.2.2",
    "prom-client": "^14.1.0",
    "redis": "^4.3.1",
    "response-time": "^2.3.2",
    "serverless-mysql": "^1.5.4",
    "svelte-local-storage-store": "^0.4.0",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6"
  }
}
